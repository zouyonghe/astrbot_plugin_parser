                    "use_proxy": {
                        "description": "使用代理",
                        "type": "bool",
                        "default": false
                    }
                }
            },
            "weibo": {
                "name": "微博解析器",
                "items": {
                    "enable": {
                        "description": "启用",
                        "type": "bool",
                        "default": true
