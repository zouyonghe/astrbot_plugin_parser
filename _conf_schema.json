                    }
                }
            },
            "ncm": {
                "name": "网易云解析器",
                "items": {
                    "enable": {
                        "description": "启用",
                        "type": "bool",
                        "default": true
                    },
                    "use_proxy": {
                        "description": "使用代理",
                        "type": "bool",
                        "default": false
                    }
                }
            },
            "nga": {
                "name": "NGA解析器",
                "items": {
