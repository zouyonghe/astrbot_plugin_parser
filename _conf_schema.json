                    }
                }
            },
            "youtube": {
                "name": "YouTube 解析器",
                "items": {
                    "enable": {
                        "description": "启用",
                        "type": "bool",
                        "default": true
                    },
                    "cookies": {
                        "description": "油管 Cookies",
                        "hint": "必填！！用于YouTube解析的登录Cookies，不填一般无法成功下载油管的视频，同时注意油管Cookie有效期较短",
                        "type": "text",
                        "default": ""
                    },
                    "use_proxy": {
                        "description": "使用代理",
                        "type": "bool",
                        "default": true
                    }
                }
            },
            "instagram": {
                "name": "Instagram 解析器",
                "items": {
                    "enable": {
